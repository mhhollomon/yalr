        { 67, TOK_SQUOTE, state_action::reduce, 0, &Parser::reduce_by_prod14},
        { 68, TOK_ARROW, state_action::reduce, 0, &Parser::reduce_by_prod18},
        { 68, TOK_RBRACE, state_action::reduce, 0, &Parser::reduce_by_prod18},
