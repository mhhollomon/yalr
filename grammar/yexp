        {
            TOK_OPTION, TOK_PARSER, TOK_LEXER, TOK_NAMESP, TOK_RULE, TOK_GOAL, TOK_TERM, TOK_SKIP, eoi, 
        },
        {
            TOK_OPTION, TOK_PARSER, TOK_LEXER, TOK_NAMESP, TOK_RULE, TOK_GOAL, TOK_TERM, TOK_SKIP, eoi, 
        },
        {
            TOK_OPTION, TOK_PARSER, TOK_LEXER, TOK_NAMESP, TOK_RULE, TOK_GOAL, TOK_TERM, TOK_SKIP, eoi, 
